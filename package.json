{
  "name": "bem-names",
  "version": "1.3.2",
  "description": "Advanced generator of BEM class names",
  "main": "dist/bem-names.js",
  "scripts": {
    "test": "NODE_ENV='test' mocha --compilers js:babel-register './test/test.js'",
    "speed": "node ./test/speed.js",
    "build": "rollup -c; NODE_ENV='production' rollup -c",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- ./test/test.js --compilers js:babel-register --recursive",
    "lint": "eslint ."
  },
  "keywords": [
    "bem",
    "names",
    "generator",
    "class",
    "classname",
    "classnames",
    "react",
    "bemNames",
    "css"
  ],
  "author": "Piotr Tomasz Monarski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Monar/bem-names.git"
  },
  "devDependencies": {
    "b_": "1.3.4",
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "bem-classname": "0.1.1",
    "bem-classnames": "1.0.7",
    "bem-cn": "2.1.3",
    "chai": "4.1.2",
    "classnames": "2.2.5",
    "eslint": "4.16.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "5.0.0",
    "rollup": "0.43.0",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "2.0.1"
  }
}
